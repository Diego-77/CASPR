<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE cables [
  <!ATTLIST solver_class id ID #REQUIRED>
]>
<simulator>
	<solver_class id="FKLeastSquares">
		<approximation_type_enum>FK_LS_ApproxOptionType</approximation_type_enum>
		<q_dot_type_enum>FK_LS_QdotOptionType</q_dot_type_enum>
	</solver_class>
	<plot_functions>
		<plot_function type="plotAngularAcceleration">
            <figure_quantity>2</figure_quantity>
        </plot_function>
        <plot_function type="plotBodyCOG">
            <figure_quantity>3</figure_quantity>
        </plot_function>
        <plot_function type="plotJointSpace">
            <figure_quantity>2</figure_quantity>
        </plot_function>
        <plot_function type="plotCableLengths">
            <figure_quantity>2</figure_quantity>
        </plot_function>
	</plot_functions>
</simulator>